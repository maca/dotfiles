"pulseaudio-ctl up"
   XF86AudioRaiseVolume

"pulseaudio-ctl down"
   XF86AudioLowerVolume

"pulseaudio-ctl mute"
  XF86AudioMute

"pulseaudio-ctl mute-input"
  XF86AudioMicMute

"xbacklight -dec 5"
   XF86MonBrightnessDown

"xbacklight -inc 5"
   XF86MonBrightnessUp

"/home/maca/bin/monitors"
  XF86Display

"/usr/bin/systemctl suspend"
  XF86WLAN