"pulseaudio-ctl up"
   XF86AudioRaiseVolume

"pulseaudio-ctl down"
   XF86AudioLowerVolume

"pulseaudio-ctl mute"
  XF86AudioMute

"pulseaudio-ctl mute-input"
  XF86AudioMicMute

"xbacklight -inc 5"
   XF86MonBrightnessUp

"xbacklight -dec 5"
   XF86MonBrightnessDown

"xbacklight -set 0"
  XF86Display
