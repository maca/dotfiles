"pamixer -i 5 --allow-boost"
   XF86AudioRaiseVolume

"pamixer -d 5"
   XF86AudioLowerVolume

"pamixer -t"
  XF86AudioMute

"pamixer --source 45 -t"
  XF86AudioMicMute

"xbacklight -dec 5"
   XF86MonBrightnessDown

"xbacklight -inc 5"
   XF86MonBrightnessUp

"/home/maca/bin/monitors"
  XF86Display

"/usr/bin/systemctl suspend"
  XF86WLAN